{
    "connected_hyphen": {
        "regex": "(?<=[0-9])-(?=[0-9])",
        "replace_to": " - "
    },
    "truncated_floats": {
        "regex": "(?<=[0-9]) \\.(?=[0-9])",
        "replace_to": "."
    },
    "num_comma_separator": {
        "regex": [
            "(?<=[0-9]) ,(?=[0-9])",
            "(?<=[0-9]), (?=[0-9])"
        ],
        "replace_to": ""
    },
    "flag_integers": {
        "regex": "(?<![\\d.])[0-9]+(?![\\d.])",
        "replace_to": "@INTEGER"
    },
    "flag_floats": {
        "regex": "[+-]?(?=\\d*[.eE])([0-9]+\\.?[0-9]*|\\.[0-9]+)([eE][+-]?[0-9]+)?",
        "replace_to": "@FLOAT"
    },
    "flag_dates": {
        "regex": "(\\d{1,2})\\s+(Jan(?:uary)?|Feb(?:ruary)?|Mar(?:ch)?|Apr(?:il)?|May|Jun(?:e)?|Jul(?:y)?|Aug(?:ust)?|Sep(?:tember)?|Oct(?:ober)?|Nov(?:ember)?|Dec(?:ember)?)\\s+(\\d{4})",
        "replace_to": "@DATE"
    },
    "flag_any_number": {
        "regex": "@INTEGER|@FLOAT",
        "replace_to": "@NUMBER"
    }
}