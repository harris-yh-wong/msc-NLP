    "integers_only": "(?<![\\d.])[0-9]+(?![\\d.])",
    "inside_brackets": "\\(([^\\)]+)\\)",
    "flag_integers": "(?<![\\d.])[0-9]+(?![\\d.])",
    "flag_floats": "[+-]?(?=\\d*[.eE])([0-9]+\\.?[0-9]*|\\.[0-9]+)([eE][+-]?[0-9]+)?",
